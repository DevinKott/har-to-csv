{
    "parser": "babel-eslint",
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:import/recommended",
        "prettier"
    ],
    "settings": {
        "react": {
            "pragma": "React",
            "version": "^16.13.1"
        }
    },
    "plugins": [
        "import",
        "react",
        "prettier"
    ],
    "env": {
        "browser": true
    },
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true,
                "trailingComma": "es5"
            }
        ],
        "prefer-const": 2
    }
}